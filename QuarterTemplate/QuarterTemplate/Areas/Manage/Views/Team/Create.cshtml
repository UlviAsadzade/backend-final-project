@model Team
@{
    ViewData["Title"] = "Create";
}


<div class="container-fluid">
    <h1>Create</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6 ">
                <label asp-for="FullName">FullName</label>
                <input asp-for="FullName" class="form-control" placeholder="FullName">
                <span class="text-danger" asp-validation-for="FullName"></span>
            </div>
            <div class="form-group col-md-6 ">
                <label asp-for="Desc">Desc</label>
                <input asp-for="Desc" class="form-control" placeholder="Desc">
                <span class="text-danger" asp-validation-for="Desc"></span>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="FacebookUrl">FacebookUrl</label>
                <input asp-for="FacebookUrl" class="form-control" placeholder="FacebookUrl">
                <span class="text-danger" asp-validation-for="FacebookUrl"></span>
            </div>
            <div class="form-group col-md-6">
                <label asp-for="TwitterUrl">TwitterUrl</label>
                <input asp-for="TwitterUrl" class="form-control" placeholder="TwitterUrl">
                <span class="text-danger" asp-validation-for="TwitterUrl"></span>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="LinkedinUrl">LinkedinUrl</label>
                <input asp-for="LinkedinUrl" class="form-control" placeholder="LinkedinUrl">
                <span class="text-danger" asp-validation-for="LinkedinUrl"></span>
            </div>
            <div class="form-group col-md-6">
                <label asp-for="YoutubeUrl">YoutubeUrl</label>
                <input asp-for="YoutubeUrl" class="form-control" placeholder="YoutubeUrl">
                <span class="text-danger" asp-validation-for="YoutubeUrl"></span>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group">
                <label asp-for="ImageFile">ImageFile</label>
                <input asp-for="ImageFile" class="form-control" placeholder="ImageFile">
                <span class="text-danger" asp-validation-for="ImageFile"></span>
            </div>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>

    </form>
</div>







