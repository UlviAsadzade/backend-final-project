@model Team
@{
    ViewData["Title"] = "Create";
}


<div class="container-fluid">
    <h1>Create</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group ">
                <label asp-for="FullName">FullName</label>
                <input asp-for="FullName" class="form-control" placeholder="FullName">
                <span class="text-danger" asp-validation-for="FullName"></span>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group ">
                <label asp-for="Desc">Desc</label>
                <input asp-for="Desc" class="form-control" placeholder="Desc">
                <span class="text-danger" asp-validation-for="Desc"></span>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group">
                <label asp-for="ImageFile">ImageFile</label>
                <input asp-for="ImageFile" class="form-control" placeholder="ImageFile">
                <span class="text-danger" asp-validation-for="ImageFile"></span>
            </div>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>

    </form>
</div>







